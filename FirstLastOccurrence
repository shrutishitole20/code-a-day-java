public class FirstLastOccurrence {
    public static void main(String[] args) {
        int[] arr = {3, 5, 2, 5, 8, 5, 10};
        int target = 5;

        int firstIndex = -1;
        int lastIndex = -1;

        // Loop through array
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] == target) {
                if (firstIndex == -1) {
                    firstIndex = i + 1; // 1-based indexing
                }
                lastIndex = i + 1; // keep updating lastIndex
            }
        }

        if (firstIndex == -1) {
            System.out.println("Element " + target + " not found in the array.");
        } else {
            System.out.println("First occurrence of " + target + " is at position: " + firstIndex);
            System.out.println("Last occurrence of " + target + " is at position: " + lastIndex);
        }
    }
}

/*
First occurrence of 5 is at position: 2
Last occurrence of 5 is at position: 6

*/
